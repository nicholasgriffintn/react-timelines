{
  "name": "react-timelines-updated",
  "version": "2.0.0-development",
  "description": "React Timelines: Updated",
  "homepage": "https://nicholasgriffintn.github.io/react-timelines/",
  "main": "lib/src/index.js",
  "scripts": {
    "build:css": "node-sass src/scss/style.scss lib/css/style.css",
    "build:js": "tsc --project tsconfig.build.json",
    "build": "npm run clean && npm run build:js && npm run build:css",
    "clean": "rimraf lib coverage",
    "coverage": "jest --coverage --collectCoverageFrom='**/*.{js,jsx,ts,tsx}'",
    "lint:js:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:js": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:prettier:fix": "prettier --write \"{e2e,src}/**/*.{js,jsx,ts,tsx}\"",
    "lint:prettier": "prettier --list-different \"{e2e,src}/**/*.{js,jsx,ts,tsx}\"",
    "lint": "npm run lint:prettier && npm run lint:js",
    "prepare": "npm run clean && npm run build",
    "test": "npm run test:build && npm run lint && npm run test:unit",
    "test:unit": "jest",
    "test:build": "tsc --project tsconfig.json --noEmit",
    "watch:css": "node-sass -w src/scss/style.scss lib/css/style.css",
    "watch:js": "tsc --project tsconfig.build.json --watch",
    "watch": "npm run watch:js & npm run watch:css",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "timeline",
    "schedule",
    "history",
    "react",
    "gantt",
    "horizontal",
    "library",
    "scroll",
    "scss",
    "sass",
    "tracks",
    "time"
  ],
  "repository": "https://github.com/nicholasgriffintn/react-timelines",
  "author": "JSainsburyPLC",
  "license": "MIT",
  "dependencies": {
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-tooltip": "5.11.1",
    "uuid": "9.0.0"
  },
  "files": [
    "lib"
  ],
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "channel": "next",
        "prerelease": true
      }
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-syntax-flow": "^7.21.4",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@types/enzyme": "3.10.13",
    "@types/jest": "29.5.1",
    "@types/node": "^18.16.2",
    "@types/prop-types": "^15.7.5",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "8.39.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "node-sass": "8.0.0",
    "prettier": "2.8.8",
    "react-dom": "18.2.0",
    "rimraf": "5.0.0",
    "semantic-release": "^21.0.1",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  }
}